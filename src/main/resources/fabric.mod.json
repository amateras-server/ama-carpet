{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "A carpet mod extension made for Amateras SMP",
  "license": "LGPL-3",
  "authors": [
    "pugur"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/amacarpet",
    "sources": "https://github.com/pugur523/ama-carpet"
  },
  "icon": "assets/ama-carpet/icon.png",
  "environment": "*",
  "accessWidener": "ama-carpet.accesswidener",
  "entrypoints": {
    "main": [
      "org.amateras_smp.amacarpet.AmaCarpet"
    ],
    "client": [
      "org.amateras_smp.amacarpet.client.AmaCarpetClient"
    ]
  },
  "mixins": [
    "ama-carpet.mixins.json",
    "ama-carpet.client.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.0",
    "fabric": "*",
    "carpet": "*",
    "minecraft": "${minecraft_dependency}"
  },
  "suggests": {
    "syncmatica": "*",
    "kyoyu": ""
  }
